		{

			System.out.println("I am flying");
		}
		else 
		{

			System.out.println("I am running");

		}


	}

	public void myCity()
	{
		System.out.println("My city is : ");
	}

	@Override
	public void mySpecialPower() {
		// TODO Auto-generated method stub

		System.out.println("My Special Power is " + specialPower );

	}

}


